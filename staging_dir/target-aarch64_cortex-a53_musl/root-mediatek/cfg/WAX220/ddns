config configure 'common'
        option enable '0'
        option update '0'
        option hostname 'domain'
        option service 'noip'
        option address '0.0.0.0'
        option username 'user'
        option password 'password'

config configure 'netgear'
        option hostname 'domain'    
        option username 'user'
        option password 'password'

config configure 'dyndns'
        option hostname 'domain'    
        option username 'user'
        option password 'password'

config configure 'pubyun'
        option hostname 'domain'    
        option username 'user'
        option password 'password'

config configure 'oray'
        option enable '0'
        option hostname 'domain'    
        option username 'user'
        option password 'password'

config configure 'noip'
        option hostname 'domain'    
        option username 'user'
        option password 'password'
