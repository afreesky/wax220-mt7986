
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth1'
	option proto 'dhcp'
	option ipaddr '192.168.0.100'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option ip6ifaceid 'eui64'
	option gateway '192.168.0.100'
	option olddns '8.8.8.8 8.8.4.4'
	option ifname 'eth1 ra0 rax0'
	option peerdns '1'

config device 'lan_dev'
	option name 'eth1'

config interface 'guest'
	option ifname ''
	option type 'bridge'
	option ipaddr '192.168.200.1'
	option netmask '255.255.255.0'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option gateway '192.168.200.1'
	option dns '0.0.0.0 0.0.0.0'
	option proto 'static'

config interface 'mgmt'
	option ifname 'ra4 rax4'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.210.1'
	option netmask '255.255.255.0'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option gateway '0.0.0.0'
	option dns '0.0.0.0 0.0.0.0'

config interface 'wifi0'
	option ifname ''
	option type 'bridge'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option proto 'static'

config interface 'wifi1'
	option ifname ''
	option type 'bridge'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option proto 'static'
	
config interface 'wifi2'
	option ifname ''
	option type 'bridge'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option proto 'static'
	
config interface 'wifi3'
	option ifname ''
	option type 'bridge'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option proto 'static'
	
config interface 'others'
	option ifname ''
	option type 'bridge'
	option defaultroute '0'
	option peerdns '0'
	option stp '0'
	option proto 'static'