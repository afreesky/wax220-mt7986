
config wifi-device 2g_5g
	option forwardMode '5G'
	option oneWifiName '0'
	option fastlaneApMode '0'
	option wpsPinAttackCheck '1'
	option wpsPinAttackNum '3'
	option conf_mode '1'
	option device 'MT7986_1_1;MT7986_1_2'
	option ssidIsolation '1' 
	option wireless1radio 'both'
	option wireless2radio 'both'
	option wireless3radio 'both'
	option wireless4radio 'both'
	option wireless5radio 'both'
	option MaxSsidNumPerRadio '5'
	option bandSteerEn '1'
	option mapMode '2'
	option bandSteerType '2'
	option bandSteerRssi '40'
	option bandSteerPersent '75'

config wifi-device '2g'
	option channel '0'
	option country 'DE'
	option igmpsnenable '1'
	option device 'MT7986_1_1'
	option ifName 'ra'
	option wifi6 '1'
	option autoRF '0'
	option txpower '100'
	option htmode 'HT20'
	option hwmode '11axg'
	option clientLimitsOn '1'
	option clientLimitsNum '64'
	option minSupportRateEnable '1'
	option minSupportRate '2'

config wifi-iface
	option ifName 'ra0'
	option radio '0'
	option ssid 'NETGEAR_WAX220_2G'
	option device 'MT7986_1_1'
	option security 'None'
	option hidden '0'
	option clientIsolation '0'
	option pmf '1'
	option auth_port '1812'
	option rekey '3600'
	option FtSupport '1'
	option l2Isolation '0'
	option guest_network '0'
	option MbssMaxStaNum '64'

